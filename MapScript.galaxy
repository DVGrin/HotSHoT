//--------------------------------------------------------------------------------------------------
// Trigger: Healing tracking for statistics
//--------------------------------------------------------------------------------------------------
bool gt_TrackHeroHealing_Func (bool testConds, bool runActions) {
    if (testConds) {
        if (!((PlayerType(EventPlayer()) == c_playerTypeUser))) {
            return false;
        }
        if (!((PlayerType(EventUnitHealLaunchPlayer()) == c_playerTypeUser))) {
            return false;
        }
        if (!((gv_GameStartBoolean == true))) {
            return false;
        }
        if (!((gv_GameEndBoolean == false))) {
            return false;
        }
    }

    if (!runActions) {
        return true;
    }

    // Healing in the base
    if (EventUnitHealEffect() == "AllHealing") {
        return true;
    }

    // Chorus E always counts as self-healing otherwise
    if (EventUnitHealEffect() == "Hero34Skill3Heal") {
        PlayerGroupLoopBegin(PlayerGroupAll());
        for (; !PlayerGroupLoopDone(); PlayerGroupLoopStep()) {
            if (!(PlayerType(PlayerGroupLoopCurrent()) == c_playerTypeUser)) {
                continue;
            }
            if (gv_SelectedCharNum[PlayerGroupLoopCurrent()] != 34) {
                continue;
            }
            if (
                    (PlayerGroupHasPlayer(GameAttributePlayersForTeam(2), PlayerGroupLoopCurrent())) &&
                    (PlayerGroupHasPlayer(GameAttributePlayersForTeam(1), EventPlayer()))
                ) {
                    continue;
            }
            if (
                    (PlayerGroupHasPlayer(GameAttributePlayersForTeam(1), PlayerGroupLoopCurrent())) &&
                    (PlayerGroupHasPlayer(GameAttributePlayersForTeam(2), EventPlayer()))
                ) {
                    continue;
            }
            if (EventPlayer() == PlayerGroupLoopCurrent()) {
                gv_PlayerStatistics[PlayerGroupLoopCurrent()].self_healing += EventUnitHealAmount();
            } else {
                gv_PlayerStatistics[PlayerGroupLoopCurrent()].healing_done += EventUnitHealAmount();
            }
            break;
        }
        PlayerGroupLoopEnd();
        return true;
    }

    if (EventPlayer() == EventUnitHealLaunchPlayer()) {
        gv_PlayerStatistics[EventUnitHealLaunchPlayer()].self_healing += EventUnitHealAmount();
    } else {
        gv_PlayerStatistics[EventUnitHealLaunchPlayer()].healing_done += EventUnitHealAmount();
    }

    return true;
}

