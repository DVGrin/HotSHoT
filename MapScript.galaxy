bool gt_TrackHeroDamage_Func (bool testConds, bool runActions) {
    if (testConds) {
        if (PlayerType(EventUnitDamageSourcePlayer()) != c_playerTypeUser) {
            return false;
        }
        if (!((gv_GameStartBoolean == true))) {
            return false;
        }
        if (!((gv_GameEndBoolean == false))) {
            return false;
        }
        if (EventPlayer() == EventUnitDamageSourcePlayer()) {
            return false;
        }
    }

    if (!runActions) {
        return true;
    }

    gv_PlayerStatistics[EventUnitDamageSourcePlayer()].self_healing += EventUnitDamageVitalsLeeched(c_unitVitalLife);

    if (PlayerType(EventPlayer()) != c_playerTypeUser) {
        return false;
    }

    gv_PlayerStatistics[EventUnitDamageSourcePlayer()].damage_dealt += EventUnitDamageAmount();
    gv_PlayerStatistics[EventPlayer()].damage_absorbed += EventUnitDamageAbsorbed();

    return true;
}

