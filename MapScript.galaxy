void gf_StatWindowUpdatePlayer (int player) {
    int i;
    int j;
    int[6] stat_sum;
    text tooltip_text;
    text[6] tooltip_text_groups;
    color stat_name_color;

    const color DEFAULT_COLOR = Color(40.0, 60.0, 100.0);
    const color NUMBER_COLOR = Color(100.0, 100.0, 100.0);
    const color PLAYERNAME_COLOR = Color(50.0, 70.0, 100.0);
    const color LABEL_COLOR = Color(100.0, 90.0, 55.0);
    const color CONSUMABLE_COLOR = Color(80.0, 80.0, 80.0);

    tooltip_text = (
        StringToText("<s val=\"PurchasePanelGroupName\">") +
        TextWithColor(PlayerName(player) + StringToText("'s stats:"), PLAYERNAME_COLOR) + // TODO: Move to external string
        StringToText("</s>") +
        StringToText("<n/><n/>") +
        StringToText("<s val=\"Header18\">")
    );

    tooltip_text_groups[c_statTooltipGroupConsumable] = TextWithColor(StringToText("Active consumables:"), LABEL_COLOR);
    tooltip_text_groups[c_statTooltipGroupOffense] = TextWithColor(StringToText("Offense:"), LABEL_COLOR);
    tooltip_text_groups[c_statTooltipGroupDefense] = TextWithColor(StringToText("Defense:"), LABEL_COLOR);
    tooltip_text_groups[c_statTooltipGroupSeal] = TextWithColor(StringToText("Seal:"), LABEL_COLOR);
    tooltip_text_groups[c_statTooltipGroupUtility] = TextWithColor(StringToText("Utility:"), LABEL_COLOR);

    for (i = 0; i < 30; i += 1) {
        if (gv_StatIncreasesBought[player][i] == 0) { // Only show bought stats
            continue;
        }
        for (j = 1; j < 6; j += 1) {
            stat_name_color = DEFAULT_COLOR;
            if (gv_ShopItemParameters[i].tooltip_group == c_statTooltipGroupConsumable) {
                stat_name_color = CONSUMABLE_COLOR;
            }
            if (gv_ShopItemParameters[i].tooltip_group == j) {
                stat_sum[j] += 1;
                tooltip_text_groups[j] += TextWithColor(
                    StringToText("<n/>  ") + gv_ShopItemParameters[i].name,
                    stat_name_color
                );
            } else {
                continue;
            }
            if (gv_ShopItemParameters[i].max_upgrades == 1) {
                continue;
            }
            TextWithColor(StringToText(": "), stat_name_color);
            tooltip_text_groups[j] += StringToText(": ");
            tooltip_text_groups[j] += TextWithColor(IntToText(gv_StatIncreasesBought[player][i]), NUMBER_COLOR);
        }
    }

    if (stat_sum[c_statTooltipGroupConsumable] == 0) {
        tooltip_text_groups[c_statTooltipGroupConsumable] = TextWithColor(
            StringToText("No active consumables"),
            LABEL_COLOR
        );
    }

    if (stat_sum[c_statTooltipGroupOffense] > 0) {
        tooltip_text += tooltip_text_groups[c_statTooltipGroupOffense];
        tooltip_text += StringToText("<n/><n/>");
    }
    if (stat_sum[c_statTooltipGroupDefense] > 0) {
        tooltip_text += tooltip_text_groups[c_statTooltipGroupDefense];
        tooltip_text += StringToText("<n/><n/>");
    }
    if (stat_sum[c_statTooltipGroupUtility] > 0) {
        tooltip_text += tooltip_text_groups[c_statTooltipGroupUtility];
        tooltip_text += StringToText("<n/><n/>");
    }
    if (stat_sum[c_statTooltipGroupSeal] > 0) {
        tooltip_text += tooltip_text_groups[c_statTooltipGroupSeal];
        tooltip_text += StringToText("<n/><n/>");
    }
    if (
            stat_sum[c_statTooltipGroupOffense] + 
            stat_sum[c_statTooltipGroupDefense] + 
            stat_sum[c_statTooltipGroupUtility] + 
            stat_sum[c_statTooltipGroupSeal] == 0
        ) {
            tooltip_text += TextWithColor(
                StringToText("No stats bought yet<n/>"),
                LABEL_COLOR
            );
    }
    tooltip_text += tooltip_text_groups[c_statTooltipGroupConsumable];

    tooltip_text += StringToText("</s>");  // Header18

    libNtve_gf_SetDialogItemTooltip(gv_ScoreDialogName[player], tooltip_text, PlayerGroupAll());
}

