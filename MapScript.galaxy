        DialogControlSetEnabled(gv_SkillLearnWindowButton[c_skillR], PlayerGroupSingle(EventPlayer()), false);
    }
    DialogControlSetVisible(gv_SkillLearnWindow, PlayerGroupSingle(EventPlayer()), true);
