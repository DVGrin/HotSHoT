int lf_KillingPlayerNumber(int victim_player, int actual_killer) {
    int i;
    int default_killer;
    int last_player_in_team;
    string[9] kill_behaviours;

    if ((PlayerType(actual_killer) == c_playerTypeUser)) {
        return actual_killer;
    }

    // Not sure this check is necessary, left it in for compatibility reasons
    if (PlayerType(actual_killer) != c_playerTypeHostile) {
        return actual_killer;
    }

    kill_behaviours[1] = "Player1Assist4";
    kill_behaviours[2] = "Player1Assist33";
    kill_behaviours[3] = "Player1Assist323";
    kill_behaviours[4] = "Player1Assist3223";
    kill_behaviours[5] = "Player1Assist32223";
    kill_behaviours[6] = "Player1Assist322223";
    kill_behaviours[7] = "Player1Assist3222223";
    kill_behaviours[8] = "Player1Assist32222222";

    i = 1;
    last_player_in_team = 4;
    default_killer = 12;
    if (PlayerGroupHasPlayer(GameAttributePlayersForTeam(1), victim_player)) {
        i = 5;
        last_player_in_team = 8;
        default_killer = 13;
    }
    for (; i <= last_player_in_team; i += 1) {
        if (PlayerStatus(gv_PlayerBayul[i]) != c_playerStatusActive) {
            continue;
        }
        if (UnitHasBehavior(gv_PlayerHero[victim_player], kill_behaviours[i])) {
            return gv_PlayerBayul[i];
        }
    }
    return default_killer;
}

string lf_HeroKillSoundLink(bool first_blood, int multikill, int killstreak) {
    if (first_blood) { return "SystemSound"; }
    if (multikill == 2) { return "SystemSound3"; }
    if (multikill == 3) { return "SystemSound32"; }
    if (multikill == 4) { return "SystemSound322"; }
    if (killstreak == 2) { return "SystemSound3234"; }        
    if (killstreak == 3) { return "SystemSound32342"; }        
    if (killstreak == 4) { return "SystemSound323422"; }        
    if (killstreak == 5) { return "SystemSound323"; }        
    if (killstreak == 6) { return "SystemSound3232"; }        
    if (killstreak == 7) { return "SystemSound3233"; }        
    if (killstreak >= 8) { return "SystemSound32332"; }        
    return "";
}

text lf_HeroKillMessage(int killer, int victim, bool first_blood, int multikill) {
    string[10] streak_message_links;
    text message = StringToText("<s val=\"HelpPanelListBoxItemTitle\">");

    if (first_blood) {
        message += TextWithColor(
            StringToText("First blood!"),  // TODO: Move to external string
            Color(0.00, 100.00, 100.00)
        );
        message += StringToText("<n/>");
    }

    // Multikills
    streak_message_links[0] = "Param/Value/AB71EABA";
    streak_message_links[1] = "Param/Value/7146803F";
    streak_message_links[2] = "Param/Value/87097627";
    // Kill streaks
    streak_message_links[3] = "Param/Value/A21CFBE1";
    streak_message_links[4] = "Param/Value/4C15A50D";
    streak_message_links[5] = "Param/Value/50A89A4B";
    streak_message_links[6] = "Param/Value/00C847D5";
    streak_message_links[7] = "Param/Value/3619623F";
    streak_message_links[8] = "Param/Value/551276B1";
    streak_message_links[9] = "Param/Value/1141AA81";

    if (multikill >= 2) {
        message += TextWithColor(
            PlayerName(killer),
            libNtve_gf_ConvertPlayerColorToColor(PlayerGetColorIndex(killer, false))
        );
        message += TextWithColor(
            StringExternal(streak_message_links[MinI(multikill - 2, 2)]),
            Color(0.00, 100.00, 100.00)
        );
        message += StringToText("<n/>");
    }

    else if (gv_HeroKillStreak[killer] >= 2) {
        message += TextWithColor(
            PlayerName(killer),
            libNtve_gf_ConvertPlayerColorToColor(PlayerGetColorIndex(killer, false))
        );
        message += TextWithColor(
            StringExternal(streak_message_links[MinI(gv_HeroKillStreak[killer] + 1, 9)]),
            Color(0.00, 100.00, 100.00)
        );
        message += StringToText("<n/>");
    }

    message += TextWithColor(
        PlayerName(killer),
        libNtve_gf_ConvertPlayerColorToColor(PlayerGetColorIndex(killer, false))
    );
    message += StringToText("(");
    message += TextWithColor(
        UnitTypeGetName(UnitGetType(gv_PlayerHero[killer])),
        Color(0.00, 100.00, 0.00)
    );
    message += StringToText(")");
    message += StringToText(" has claimed");  // TODO: Move to external string
    message += StringToText("<n/>");
    TextWithColor(
        PlayerName(victim),
        libNtve_gf_ConvertPlayerColorToColor(PlayerGetColorIndex(victim, false))
    );
    message += StringToText("(");
    message += TextWithColor(
        UnitTypeGetName(UnitGetType(gv_PlayerHero[victim])),
        Color(0.00, 100.00, 0.00)
    );
    message += StringToText(")");
    message += StringToText("'s life<n/>");  // TODO: Move to external string

    message += StringToText("</s>");  // HelpPanelListBoxItemTitle
    return message;
}

void lf_HeroPassiveEffectsOnDeath(int player, unit hero) {
    // Kaion particle effects
    if (gv_SelectedCharNum[player] == 24) {
        PlayerCreateEffectPoint(player, "Hero24Death", UnitGetPosition(hero));
    }
    // Voltma passive AD
    else if (gv_SelectedCharNum[player] == 54) {
        UnitBehaviorRemove(hero, "Hero54Passive", (TechTreeBehaviorCount(player, "Hero54Passive", c_techCountInProgressOrBetter) / 3));
        UnitBehaviorRemove(hero, "Hero54Passive2", (TechTreeBehaviorCount(player, "Hero54Passive2", c_techCountInProgressOrBetter) / 3));
        UnitBehaviorRemove(hero, "Hero54Passive22", (TechTreeBehaviorCount(player, "Hero54Passive22", c_techCountInProgressOrBetter) / 3));
        UnitBehaviorRemove(hero, "Hero54Passive222", (TechTreeBehaviorCount(player, "Hero54Passive222", c_techCountInProgressOrBetter) / 3));
    }
}

void lf_HeroPassiveEffectsOnKill(int player, unit hero) {
    // Galvaron W passive
    if ((gv_SelectedCharNum[player] == 32) && (gv_SkillLevels[player][c_skillW] >= 1)) {
        PlayerCreateEffectPoint(player, "Hero32Skill2G22", PointFromId(1));
    }
    // Shuirin passive
    else if ((gv_SelectedCharNum[player] == 46) && (TechTreeUpgradeCount(player, "SpellUp222233322343", c_techCountCompleteOnly) < 50)) {
        UnitBehaviorAdd(hero, "Hero46Passive", hero, 2);
        TechTreeUpgradeAddLevel(player, "SpellUp222233322343", 2);
    }
    // Demol passive
    else if ((gv_SelectedCharNum[player] == 66) && (TechTreeUpgradeCount(player, "SpellUp222233322722322", c_techCountCompleteOnly) < 24)) {
        TechTreeUpgradeAddLevel(player, "SpellUp222233322722322", 1);
        // TODO: Move this check from gt_TrHero66PassiveOn to separate function
        // TODO: There is no need for it to be a trigger
        if (((TechTreeUpgradeCount(player, "SpellUp222233322722322", c_techCountCompleteOnly) == 4) || (TechTreeUpgradeCount(player, "SpellUp222233322722322", c_techCountCompleteOnly) == 8) || (TechTreeUpgradeCount(player, "SpellUp222233322722322", c_techCountCompleteOnly) == 12) || (TechTreeUpgradeCount(player, "SpellUp222233322722322", c_techCountCompleteOnly) == 16) || (TechTreeUpgradeCount(player, "SpellUp222233322722322", c_techCountCompleteOnly) == 24))) {
            PlayerCreateEffectPoint(player, "Hero66Passive", PointFromId(1));
        }
    }
    // Drager jump reset
    else if ((gv_SelectedCharNum[player] == 70) && (UnitHasBehavior(gv_PlayerHero[player], "Hero70Skill2cool") == true)) {
        UnitCreateEffectUnit(hero, "Hero70Skill2Dong3", hero);
    }
}

bool gt_TrHeroDeath2_Func (bool testConds, bool runActions) {
    int bounty = 0;
    int multikill = 0;
    int killing_player;
    bool first_blood = false;

    if (testConds) {
        if (!((PlayerType(EventPlayer()) == c_playerTypeUser))) {
            return false;
        }
        if (!((EventUnit() == gv_PlayerHero[EventPlayer()]))) {
            return false;
        }
        if (!((gv_HeroDeathBoolean[EventPlayer()] == false))) {
            return false;
        }
        if (!((gv_GameEndBoolean == false))) {
            return false;
        }
    }

    if (!runActions) {
        return true;
    }

    gv_HeroDeathBoolean[EventPlayer()] = true;

    // Proward passive
    if ((UnitGetType(EventUnit()) == "Hero19")) {
        libNtve_gf_MakeUnitInvulnerable(gv_PlayerHero[EventPlayer()], true);
        UnitRevive(gv_PlayerHero[EventPlayer()]);
        UnitBehaviorAdd(gv_PlayerHero[EventPlayer()], "Hero19Passive", gv_PlayerHero[EventPlayer()], 1);
        UnitSetPropertyFixed(gv_PlayerHero[EventPlayer()], c_unitPropLifePercent, 100.0);
        UnitSetPropertyFixed(gv_PlayerHero[EventPlayer()], c_unitPropEnergyPercent, 100.0);
        UnitClearSelection(EventPlayer());
        UnitSelect(gv_PlayerHero[EventPlayer()], EventPlayer(), true);
    }

    // Find out the killing player, heroes take priority over non-player entities
    killing_player = lf_KillingPlayerNumber(EventPlayer(), libNtve_gf_KillingPlayer());
    gv_HeroKillPlayer[EventPlayer()] = killing_player;

    // Transfer the jungle buffs if any
    if (((gv_HeroBuffIng[EventPlayer()][0] >= 1) || (gv_HeroBuffIng[EventPlayer()][1] >= 1))) {
        gf_HrHeroBuffPut();
    }

    lf_HeroPassiveEffectsOnDeath(EventPlayer(), EventUnit());

    if ((PlayerType(killing_player) == c_playerTypeUser)) {
        if (gv_HeroFirstKill == false) {
            gv_HeroFirstKill = true;
            first_blood = true;
            SoundPlay(SoundLink("SystemSound", -1), PlayerGroupAll(), 200.0, 0.0);
            bounty += 100;
        }

        lf_HeroPassiveEffectsOnKill(killing_player, gv_PlayerHero[killing_player]);

        // Behaviours to track a potential multikill
        // UnitBehaviorAdd(gv_PlayerHero[killing_player], "PlayerGKill", gv_PlayerHero[killing_player], 1);
        UnitBehaviorAdd(gv_PlayerHero[killing_player], "PlayerGKill2", gv_PlayerHero[killing_player], 1);
        multikill = UnitBehaviorCount(gv_PlayerHero[killing_player], "PlayerGKill2");

        // TODO: Get rid of gv_HeroKillMoney?
        // Base bounty
        bounty += gv_HeroKillMoney[EventPlayer()][0];

        // Bounty for killing someone who's higher level than you
        if (gv_PlayerLevelAndKDA[EventPlayer()][0] > gv_PlayerLevelAndKDA[killing_player][0]) {
            bounty += gv_HeroKillMoney[EventPlayer()][1];
        }

        // Bounties for kill or death streaks
        if (gv_HeroKillStreak[EventPlayer()] < 0) {
            bounty += 2 * gv_HeroKillStreak[EventPlayer()] * (gv_PlayerLevelAndKDA[EventPlayer()][0] - 1);
        }
        else if (gv_HeroKillStreak[EventPlayer()] == 2) {
            bounty += 50;
        }
        else if (gv_HeroKillStreak[EventPlayer()] > 2) {
            bounty += 25 * (gv_HeroKillStreak[EventPlayer()] + 1);
        }

        // You can't get less than 60 minerals for a kill
        bounty = MaxI(60, bounty);

        // Give the minerals and update statistics
        PlayerModifyPropertyInt(killing_player, c_playerPropMinerals, c_playerPropOperAdd, bounty);
        gv_PlayerStatistics[EventPlayer()].minerals_lost += bounty;

        // Display the message with the minerals earned to the killer
        UIDisplayMessage(
            PlayerGroupSingle(killing_player),
            c_messageAreaSubtitle,
            (
                StringExternal("Param/Value/FB1959EE") + 
                TextWithColor(StringExternal("Param/Value/772AC933"), Color(0.00, 87.45, 100.00)) + 
                TextWithColor(IntToText(bounty), Color(0.00, 87.45, 100.00)) + 
                TextWithColor(StringExternal("Param/Value/FABCA002"), Color(0.00, 87.45, 100.00)) +
                StringExternal("Param/Value/3453FA0E")
            )
        );

        // Increment the kill count and killstreaks
        gv_PlayerLevelAndKDA[killing_player][1] += 1;
        gv_HeroKillStreak[killing_player] = MaxI(1, gv_HeroKillStreak[killing_player] + 1);
        gv_HeroKillStreak[EventPlayer()] = MinI(-1, gv_HeroKillStreak[EventPlayer()] - 1);

        // Refresh Ghost duration if it's active
        if (UnitHasBehavior(gv_PlayerHero[killing_player], "SpellDong4") == true) {
            UnitCreateEffectUnit(gv_PlayerHero[killing_player], "Spelldong22", gv_PlayerHero[killing_player]);
        }

        // TODO: Check if this effect needs changes, specifically HeroKillGold part
        // Give the kill experience
        // PlayerCreateEffectUnit(EventPlayer(), "HeroKillMo", gv_PlayerHero[killing_player]);
        PlayerCreateEffectUnit(EventPlayer(), "HeroKill", gv_PlayerHero[killing_player]);

        // Set the new score dialog values
        libNtve_gf_SetDialogItemText(
            gv_ScoreDialogTotalScore[killing_player],
            (
                StringExternal("Param/Value/6B201FB2") + 
                IntToText(gv_PlayerLevelAndKDA[killing_player][1]) + 
                StringExternal("Param/Value/D24522CA") + 
                IntToText(gv_PlayerLevelAndKDA[killing_player][2]) + 
                StringExternal("Param/Value/B3E4C0D7") + 
                IntToText(gv_PlayerLevelAndKDA[killing_player][3]) + 
                StringExternal("Param/Value/0D0DA65C")
            ),
            PlayerGroupAll()
        );

        // Increment the team kill count on the scoreboard
        if ((PlayerGroupHasPlayer(GameAttributePlayersForTeam(1), killing_player) == true)) {
            gv_PlayerProfileTeamKillNum[0] += 1;
            libNtve_gf_SetDialogItemText(gv_ScoreDialogTextB[4], (StringExternal("Param/Value/46715145") + IntToText(gv_PlayerProfileTeamKillNum[0]) + StringExternal("Param/Value/211C87AC")), PlayerGroupAll());
        }
        else if ((PlayerGroupHasPlayer(GameAttributePlayersForTeam(2), killing_player) == true)) {
            gv_PlayerProfileTeamKillNum[1] += 1;
            libNtve_gf_SetDialogItemText(gv_ScoreDialogTextB[5], (StringExternal("Param/Value/1CD6E931") + IntToText(gv_PlayerProfileTeamKillNum[1]) + StringExternal("Param/Value/B521C143")), PlayerGroupAll());
        }

        // Display the kill message
        UIDisplayMessage(
            PlayerGroupAll(),
            c_messageAreaDebug,
            lf_HeroKillMessage(killing_player, EventPlayer(), first_blood, multikill)
        );

        // Play the kill sound
        SoundPlay(
            SoundLink(lf_HeroKillSoundLink(first_blood, multikill, gv_HeroKillStreak[killing_player]), -1),
            PlayerGroupAll(), 200.0, 0.0
        );
    }

    // Increment the death count
    gv_PlayerLevelAndKDA[EventPlayer()][2] += 1;

    // Score dialog changes for the victim
    libNtve_gf_SetDialogItemText(
        gv_ScoreDialogTotalScore[EventPlayer()], 
        (
            StringExternal("Param/Value/4C5AC417") + 
            IntToText(gv_PlayerLevelAndKDA[EventPlayer()][1]) + 
            StringExternal("Param/Value/E32F94F1") + 
            IntToText(gv_PlayerLevelAndKDA[EventPlayer()][2]) + 
            StringExternal("Param/Value/784ECCE0") + 
            IntToText(gv_PlayerLevelAndKDA[EventPlayer()][3]) + 
            StringExternal("Param/Value/43A6870C")
        ),
        PlayerGroupAll()
    );
    // Change the name colour to red
    libNtve_gf_SetDialogItemText(
        gv_ScoreDialogName[EventPlayer()],
        (
            StringExternal("Param/Value/70CACF72") + 
            PlayerName(EventPlayer()) + 
            StringExternal("Param/Value/152670BB")
        ),
        PlayerGroupAll()
    );
    // Make the hero image red and slightly transparent
    libNtve_gf_SetDialogItemColor(gv_ScoreDialogHeroImage[EventPlayer()], Color(100.00, 0.00, 0.00), PlayerGroupAll());
    DialogControlFadeTransparency(gv_ScoreDialogHeroImage[EventPlayer()], PlayerGroupAll(), 0.0, 50.0);
    // Enable the revive timer countdown
    DialogControlSetVisible(gv_ScoreDialogReviveTime[EventPlayer()], PlayerGroupAll(), true);

    // TODO: Look if those two functions can be combined
    if ((PlayerGroupHasPlayer(GameAttributePlayersForTeam(1), EventPlayer()) == true) && ((UnitHasBehavior(gv_PlayerHero[EventPlayer()], "Player1Assist3222") == true) || (UnitHasBehavior(gv_PlayerHero[EventPlayer()], "Player1Assist32222") == true) || (UnitHasBehavior(gv_PlayerHero[EventPlayer()], "Player1Assist322222") == true) || (UnitHasBehavior(gv_PlayerHero[EventPlayer()], "Player1Assist3222222") == true))) {
        gf_HrAssistantBonusTeam2();
    }
    else if ((PlayerGroupHasPlayer(GameAttributePlayersForTeam(2), EventPlayer()) == true) && ((UnitHasBehavior(gv_PlayerHero[EventPlayer()], "Player1Assist") == true) || (UnitHasBehavior(gv_PlayerHero[EventPlayer()], "Player1Assist3") == true) || (UnitHasBehavior(gv_PlayerHero[EventPlayer()], "Player1Assist32") == true) || (UnitHasBehavior(gv_PlayerHero[EventPlayer()], "Player1Assist322") == true))) {
        gf_HrAssistantBonusTeam1();
    }

    // A message in chat if the whole team is dead
    // TODO: Refactor this function
    if ((((PlayerGroupHasPlayer(GameAttributePlayersForTeam(1), EventPlayer()) == true) && (gv_HeroDeathBoolean[gv_PlayerBayul[1]] == true) && (gv_HeroDeathBoolean[gv_PlayerBayul[2]] == true) && (gv_HeroDeathBoolean[gv_PlayerBayul[3]] == true) && (gv_HeroDeathBoolean[gv_PlayerBayul[4]] == true) && (gv_HeroAllKill[0] == false) && (PlayerGroupCount(gv_TeamPlayerGroup[0]) >= 2)) || ((PlayerGroupHasPlayer(GameAttributePlayersForTeam(2), EventPlayer()) == true) && (gv_HeroDeathBoolean[gv_PlayerBayul[5]] == true) && (gv_HeroDeathBoolean[gv_PlayerBayul[6]] == true) && (gv_HeroDeathBoolean[gv_PlayerBayul[7]] == true) && (gv_HeroDeathBoolean[gv_PlayerBayul[8]] == true) && (gv_HeroAllKill[1] == false) && (PlayerGroupCount(gv_TeamPlayerGroup[1]) >= 2)))) {
        gf_HrHeroAllKillMessage();
    }

    if ((UnitGetType(EventUnit()) != "Hero19")) {
        TriggerExecute(gt_TrHeroReviveStand, true, false);
    }

    return true;
}

