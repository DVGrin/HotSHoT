            if ((gv_SelectedCharNum[gv_PlayerBayul[i]] == 6)) {
                TechTreeUpgradeAddLevel(gv_PlayerBayul[i], "SpellUp222233322234", 1);
            }
            if ((gv_SelectedCharNum[gv_PlayerBayul[i]] == 9)) {
