void gf_HrCharSelectDialogHeroBtnMake () {
    int i;
    int x = 0; int y = 0;
    int page = 0;
    int x_start = 30;
    int x_step = 105;
    int y_start = 80;
    int y_step = 100;

    // Index of a page in gv_CharChooseDialog
    int[5] page_dialog_index;
    int[85] image_offset;

    page_dialog_index[0] = 0;
    page_dialog_index[1] = 3;
    page_dialog_index[2] = 5;
    page_dialog_index[3] = 6;
    page_dialog_index[4] = 7;

    for (i = 0; i < 85; i += 1) {
        image_offset[i] = 10;
    }
    // Exceptions to preserve the look from the MaxStorm's version
    image_offset[31] = 5;
    image_offset[32] = 5;
    image_offset[36] = 7;
    image_offset[42] = 5;
    image_offset[47] = 5;
    image_offset[52] = 5;
    image_offset[53] = 7;
    image_offset[59] = 7;
    image_offset[67] = 15;
    image_offset[70] = 7;

    for (i = 0; i < 85; i += 1) {
        libNtve_gf_CreateDialogItemButton(
            gv_CharChooseDialog[page_dialog_index[page]],
            100, 100, c_anchorTopLeft, x_start + x_step * x, y_start + y_step * y,
            gv_HeroInformation[i].selection_tooltip,
            StringToText(""),
            "Assets\\Textures\\ui_techpurchase_techbutton_selected_normaloverpressedover.dds"
        );
        libNtve_gf_SetDialogItemImage(
            DialogControlLastCreated(),
            "Assets\\Textures\\ui_techpurchase_techbutton_hired_normaloverpressedover.dds",
            PlayerGroupAll()
        );
        libNtve_gf_SetDialogItemImageType(
            DialogControlLastCreated(),
            c_triggerImageTypeBorder,
            PlayerGroupAll()
        );
        gv_CharChooseWindowBtn[i] = DialogControlLastCreated();

        if (i < gv_TotalHeroNum) {
            libNtve_gf_CreateDialogItemImage(
                gv_CharChooseDialog[page_dialog_index[page]],
                100 - image_offset[i] * 2,
                100 - image_offset[i] * 2,
                c_anchorTopLeft,
                x_start + x_step * x + image_offset[i],
                y_start + y_step * y + image_offset[i],
                StringToText(""),
                gv_HeroInformation[i].image,
                c_triggerImageTypeNormal, false,
                Color(100, 100, 100),
                c_triggerBlendModeNormal
            );
            gv_CharChooseWindowImage[i] = DialogControlLastCreated();
        }

        x += 1;
        if ((x == 5) && (y == 2)) {
            x = 0;
            y = 0;
            page += 1;
        }
        if (x >= 6) {
            x = 0;
            y += 1;
        }
    }

    for (i = 0; i < 5; i += 1) {
        libNtve_gf_CreateDialogItemButton(
            gv_CharChooseDialog[page_dialog_index[i]],
            100, 100, c_anchorTopLeft, 555, 280,
            StringExternal("Param/Value/7ABB05AB"),
            StringToText(""),
            "Assets\\Textures\\ui_techpurchase_techbutton_selected_normaloverpressedover.dds"
        );

        libNtve_gf_SetDialogItemImage(
            DialogControlLastCreated(),
            "Assets\\Textures\\ui_techpurchase_techbutton_hired_normaloverpressedover.dds",
            PlayerGroupAll()
        );
        libNtve_gf_SetDialogItemImageType(
            DialogControlLastCreated(),
            c_triggerImageTypeBorder,
            PlayerGroupAll()
        );
        gv_CharChooseWindowRandomBtn[i] = DialogControlLastCreated();

        libNtve_gf_CreateDialogItemImage(
            gv_CharChooseDialog[page_dialog_index[i]],
            80, 80, c_anchorTopLeft, 565, 290,
            StringToText(""),
            "Assets\\Textures\\btn-tips-tutorial.dds",
            c_triggerImageTypeNormal, false,
            Color(100, 100, 100),
            c_triggerBlendModeNormal
        );
        gv_CharChooseWindowRandomImage[i] = DialogControlLastCreated();
    }
}

