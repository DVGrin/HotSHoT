bool gt_EndGameForPlayer_Func (bool testConds, bool runActions) {
    if (testConds) {
        if (!((gv_GameEndBoolean == true))) {
            return false;
        }
        if (gv_WinningTeam == 0) {
            return false;
        }
    }

    if (!runActions) {
        return true;
    }

    if (PlayerGroupHasPlayer(GameAttributePlayersForTeam(3), EventPlayer())) {
        GameOver(EventPlayer(), c_gameOverTie, true, true);
        return true;
    }

    if (PlayerGroupHasPlayer(GameAttributePlayersForTeam(gv_WinningTeam), EventPlayer())) {
        GameOver(EventPlayer(), c_gameOverVictory, true, true);
    } else {
        GameOver(EventPlayer(), c_gameOverDefeat, true, true);
    }

    return true;
}

