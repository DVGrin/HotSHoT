void gf_UpdatePlayerStatisticsWindow() {
    int i;
    int player;
    int participated_in;

    for (i = 0; i < 8; i += 1) {
        player = gv_PlayerBayul[i + 1];
        if (!gv_PlayerStatistics[player].display_stats) {
            continue;
        }

        libNtve_gf_SetDialogItemText(
            gv_PlayerStatisticsWindowNumber[i][c_statisticsColumnLabelKDA],
            (
                IntToText(gv_PlayerLevelAndKDA[player][1]) + 
                StringToText("/") +
                IntToText(gv_PlayerLevelAndKDA[player][2]) + 
                StringToText("/") +
                IntToText(gv_PlayerLevelAndKDA[player][3])         
            ),
            PlayerGroupAll()
        );
        libNtve_gf_SetDialogItemText(
            gv_PlayerStatisticsWindowNumber[i][c_statisticsColumnLabelDamageDone],
            FixedToText(gv_PlayerStatistics[player].damage_dealt, 0),
            PlayerGroupAll()
        );
        libNtve_gf_SetDialogItemText(
            gv_PlayerStatisticsWindowNumber[i][c_statisticsColumnLabelDamageReceived],
            FixedToText(gv_PlayerStatistics[player].damage_absorbed, 0),
            PlayerGroupAll()
        );
        libNtve_gf_SetDialogItemText(
            gv_PlayerStatisticsWindowNumber[i][c_statisticsColumnLabelHealing],
            FixedToText(gv_PlayerStatistics[player].healing_done, 0),
            PlayerGroupAll()
        );
        libNtve_gf_SetDialogItemText(
            gv_PlayerStatisticsWindowNumber[i][c_statisticsColumnLabelSelfHealing],
            FixedToText(gv_PlayerStatistics[player].self_healing, 0),
            PlayerGroupAll()
        );
        libNtve_gf_SetDialogItemText(
            gv_PlayerStatisticsWindowNumber[i][c_statisticsColumnLabelResources],
            (
                StringToText("<IMG path=\"Assets\\Textures\\icon-mineral-protoss.dds\" height=\"18\" width=\"18\"/> ") +
                IntToText(gv_PlayerStatistics[player].minerals_spent) +
                StringToText("</n><IMG path=\"Assets\\Textures\\icon-gas-protoss.dds\" height=\"18\" width=\"18\"/> ") +
                IntToText(gv_PlayerStatistics[player].gas_spent)
            ),
            PlayerGroupAll()
        );
        libNtve_gf_SetDialogItemText(
            gv_PlayerStatisticsWindowNumber[i][c_statisticsColumnLabelWards],
            IntToText(gv_PlayerStatistics[player].wards_placed),
            PlayerGroupAll()
        );
        libNtve_gf_SetDialogItemText(
            gv_PlayerStatisticsWindowNumber[i][c_statisticsColumnLabelTimeSpentDead],
            IntToText(gv_PlayerStatistics[player].time_spent_dead),
            PlayerGroupAll()
        );
        libNtve_gf_SetDialogItemText(
            gv_PlayerStatisticsWindowNumber[i][c_statisticsColumnLabelMineralsLost],
            IntToText(gv_PlayerStatistics[player].minerals_lost),
            PlayerGroupAll()
        );
        
        participated_in = (gv_PlayerLevelAndKDA[player][1] + gv_PlayerLevelAndKDA[player][3]) * 100;
        if ((i < 4) && (gv_PlayerProfileTeamKillNum[0] > 0)) {
            participated_in /= gv_PlayerProfileTeamKillNum[0];
        }
        if ((i >= 4) && (gv_PlayerProfileTeamKillNum[1] > 0)) {
            participated_in /= gv_PlayerProfileTeamKillNum[1];
        }
        libNtve_gf_SetDialogItemText(
            gv_PlayerStatisticsWindowNumber[i][c_statisticsColumnLabelKillParticipation],
            IntToText(participated_in) + StringToText("%"),
            PlayerGroupAll()
        );
    }
}

