//--------------------------------------------------------------------------------------------------
bool gt_Hero33Skill2Vision_Func (bool testConds, bool runActions) {
    int i;
    bool share_vision;
    playergroup allied_team;
    int allied_minions;

    if (!runActions) {
        return true;
    }

    // Get the team as a playergroup
    if ((PlayerGroupHasPlayer(GameAttributePlayersForTeam(1), UnitGetOwner(EventUnit())) == true)) {
        allied_team = GameAttributePlayersForTeam(1);
        allied_minions = 12;
    } else if ((PlayerGroupHasPlayer(GameAttributePlayersForTeam(2), UnitGetOwner(EventUnit())) == true)) {
        allied_team = GameAttributePlayersForTeam(2);
        allied_minions = 13;
    } else {
        return false;
    }

    // Smoke screen on
    if ((UnitBehaviorCount(EventUnit(), "Hero33Skill3impact3") >= 1)) {
        share_vision = false;
    }
    // Smoke screen off
    else if ((UnitBehaviorCount(EventUnit(), "Hero33Skill3impact3") == 0)) {
        share_vision = true;
    }

    PlayerGroupLoopBegin(allied_team);
    for (; !PlayerGroupLoopDone(); PlayerGroupLoopStep()) {
        if (PlayerGroupLoopCurrent() == UnitGetOwner(EventUnit())) {
            continue;
